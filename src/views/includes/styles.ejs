<link rel="stylesheet" href="/css/dark.css" />
<style>
  html,
  body {
    height: 100%;
  }
  .content-main {
    display: flex;
    flex-direction: column;
    height: 100%;
  }
  .grow {
    flex-grow: 1;
  }
  .footer {
    margin-top: 1rem;
  }
  /* banners */
  .banner {
    min-height: 2rem;
    background: solid;
    text-align: center;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  .banner.is-danger {
    background: hsl(348, 100%, 61%);
  }
  .banner.is-primary {
    background: hsl(171, 100%, 41%);
  }
  .banner.is-link {
    background: hsl(217, 71%, 53%);
  }
  .banner.is-info {
    background: hsl(204, 86%, 53%);
  }
  .banner.is-success {
    background: hsl(141, 53%, 53%);
  }
  .banner.is-warning {
    background: hsl(48, 100%, 67%);
    color: rgba(0, 0, 0, 0.7) !important;
  }
</style>
<%- style %>
